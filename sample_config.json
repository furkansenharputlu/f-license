{
  "port": 4242,
  "control_api_secret": "admin123",
  "admin_secret": "admin123",
  "db_options": {
    "file":{
      "dir": "./storage"
    },
    "mongo": {
      "url": "mongodb://localhost:27017",
      "name": "f-license"
    }
  },
  "apps": {
    "test-app": {
      "alg": "HS512",
      "signature": {
        "hmac_secret": "test-hmac-secret",
        "rsa_private_key_file": "sample_private_key.pem",
        "rsa_public_key_file": "sample_public_key.pem"
      }
    }
  },
  "default_keys": {
    "hmac_secret": {
      "raw": "test-default-hmac-secret"
    },
    "rsa_private_key":{
      "id": "",
      "file_path": "sample_private_key.pem"
    },
    "rsa_public_key": {
      "id": "",
      "file_path": "sample_public_key.pem"
    }
  },
  "server_options": {
    "enable_tls": true,
    "cert_file": "sample_public_key.pem",
    "key_file": "sample_private_key.pem",
    "tls_config": {
      "ServerName": "localhost"
    }
  }
}